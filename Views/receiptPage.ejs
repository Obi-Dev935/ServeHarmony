<!DOCTYPE html>
<html lang="en">
<head>
  <!-- general meta tags -->
  <meta charset="utf-8">
  <meta name="author" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- site specific tags -->
  <title></title>
  <!-- style sheets -->
  <link rel="stylesheet" href="css-reset.css">
  <link rel="stylesheet" href="/css/styles.css">
  <!-- javascript files -->
</head>
<body>
  <header>
    
  </header>
  <main>
    <section>
        <% if (orderData && orderData.menuitems) { %>
            <ul class="grid">
            <% orderData.menuitems.forEach(order => { %>
                <li class="card">
                    <img src="/images/<%= order.itemImg %>" id="Image">
                    <h3><%= order.itemName %></h3>
                    <p>Price: <%= order.itemPrice %> SAR</p>
                </li>
            <% }); %>
            </ul>
        <% }; %>
    </section>
  </main>
  <footer>
    
  </footer>
</body>
</html>